{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "nixpacks",
    "nixpacksVersion": "latest"
  },
  "deploy": {
    "numReplicas": 1,
    "startCommand": "npm start",
    "healthcheckPath": "/health",
    "healthcheckInterval": 10,
    "healthcheckTimeout": 5
  }
}
